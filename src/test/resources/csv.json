{
  "name": "voy_sch",
  "fromDS": {
    "url": "jdbc:mysql://localhost:3306/tacsp",
    "username": "root",
    "password": "password"
  },
  "toDS": {
    "url": "jdbc:mysql://localhost:3306/kappals_dev",
    "username": "root",
    "password": "password"
  },
  "extract": {
    "csv" : {
      "file" : "./test.csv",
      "columns" : ["code", "name", "username"]
    }
  },
  "transform": {
    "columns": [
      {
        "name" : "ref_fk",
        "sql" : "select id from visual_user where code = :username"
      }
    ]
  },
  "load": [
    {
      "pre": {
        "csv" : {
          "file" : "./test.csv",
          "columns" : ["code", "name", "username"]
        }
      },
      "table": "test_csv",
      "columns": [
        {
          "name": "id",
          "auto": true
        },
        {
          "name": "code",
          "ref": "code",
          "nk" : true
        },
        {
          "name": "name",
          "ref": "name"
        },
        {
          "name": "ref_fk",
          "generator": "select max(ref_fk)+1 from test_csv",
          "is_update": false
        },
        {
          "name": "active_yn",
          "value": 1
        }
      ],
      "mode": "MERGE",
      "post": {
        "csv" : {
          "file" : "./test.csv",
          "columns" : ["code", "name", "username"]
        }
      }
    }
  ]
}
